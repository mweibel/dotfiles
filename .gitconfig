[user]
	name = Michael Weibel
	email = michael.weibel@gmail.com
[color]
	ui = true
[alias]
	st = status -sb
	co = checkout
	ci = commit
	br = branch
	l = log --graph --full-history --pretty=format:'%C(red)%h%Creset - %C(yellow)%s %C(green)(%an, %cr)%C(white)%d%C(reset)' --abbrev-commit --date=relative
	graph = log --graph --oneline --all
[core]
	editor = /usr/bin/vim
	quotepath = false
	ignorecase = false
	excludesfile = /Users/wemi/.gitignore
	trustctime = false
[push]
	default = current
